{
  "name": "ostark/craft-plugin-commands",
  "description": "Manage Craft plugins from the cli",
  "version": "1.1.0",
  "type": "yii2-extension",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "yii2-extension",
    "plugins"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oliver Stark",
      "homepage": "https://www.fortrabbit.com"
    }
  ],
  "support": {
    "email": "os@fortrabbit.com",
    "issues": "https://github.com/ostark/craft-plugin-commands/issues",
    "source": "https://github.com/ostark/craft-plugin-commands",
    "docs": "https://github.com/ostark/craft-plugin-commands"
  },
  "require": {
    "craftcms/cms": "^3.2.0 || ^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "ostark\\PluginCommands\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "ostark\\PluginCommands\\BootstrapPluginCommands"
  }
}
